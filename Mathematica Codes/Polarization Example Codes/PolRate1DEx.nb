Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "We", " ", "assume", " ", "that", " ", "the", " ", "first", " ", "3", " ", 
    "bands", " ", "are", " ", "filled"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"n", "=", "1"}], ",", 
    RowBox[{"n", "<=", "ntot"}], ",", 
    RowBox[{"n", "++"}], ",", "\[IndentingNewLine]", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"i", "=", "1"}], ",", 
      RowBox[{"i", "<=", "Ntot"}], ",", 
      RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"k", ":=", 
        RowBox[{
         RowBox[{"(", 
          FractionBox[
           RowBox[{
            RowBox[{"2", "*", "i"}], "-", "Ntot", "-", "1"}], 
           RowBox[{"2", "*", "Ntot"}]], ")"}], "*", "2", "*", 
         FractionBox["Pi", "L"]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"vals", "=", 
        RowBox[{"NDEigenvalues", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{"-", 
              RowBox[{"Laplacian", "[", 
               RowBox[{
                RowBox[{"u", "[", "x", "]"}], ",", 
                RowBox[{"{", "x", "}"}]}], "]"}]}], "-", 
             RowBox[{"2", "*", "I", "*", "k", "*", 
              RowBox[{"D", "[", 
               RowBox[{
                RowBox[{"u", "[", "x", "]"}], ",", "x"}], "]"}]}], "+", 
             RowBox[{
              SuperscriptBox["k", "2"], "*", 
              RowBox[{"u", "[", "x", "]"}]}], "+", 
             RowBox[{
              RowBox[{"V", "[", 
               RowBox[{"x", ",", "\[Lambda]1"}], "]"}], "*", 
              RowBox[{"u", "[", "x", "]"}]}]}], ",", 
            RowBox[{
             RowBox[{"u", "[", "a", "]"}], "\[Equal]", 
             RowBox[{"u", "[", 
              RowBox[{"a", "+", "L"}], "]"}]}]}], "}"}], ",", 
          RowBox[{"u", "[", "x", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", "a", ",", 
            RowBox[{"a", "+", "L"}]}], "}"}], ",", "ntot"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"ufun", "=", 
          RowBox[{"NDSolve", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"-", 
                 RowBox[{"Laplacian", "[", 
                  RowBox[{
                   RowBox[{"u", "[", "x", "]"}], ",", 
                   RowBox[{"{", "x", "}"}]}], "]"}]}], "-", 
                RowBox[{"2", "*", "I", "*", "k", "*", 
                 RowBox[{"D", "[", 
                  RowBox[{
                   RowBox[{"u", "[", "x", "]"}], ",", "x"}], "]"}]}], "+", 
                RowBox[{
                 SuperscriptBox["k", "2"], "*", 
                 RowBox[{"u", "[", "x", "]"}]}], "+", 
                RowBox[{
                 RowBox[{"V", "[", 
                  RowBox[{"x", ",", "\[Lambda]"}], "]"}], "*", 
                 RowBox[{"u", "[", "x", "]"}]}]}], "\[Equal]", 
               RowBox[{
                RowBox[{"vals", "[", 
                 RowBox[{"[", "n", "]"}], "]"}], "*", 
                RowBox[{"u", "[", "x", "]"}]}]}], ",", 
              RowBox[{
               RowBox[{"u", "[", "a", "]"}], "\[Equal]", 
               RowBox[{"u", "[", 
                RowBox[{"a", "+", "L"}], "]"}]}], ",", 
              RowBox[{
               RowBox[{"u", "[", "a", "]"}], "\[Equal]", "1.2"}]}], "}"}], 
            ",", 
            RowBox[{"u", "[", "x", "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"x", ",", "a", ",", 
              RowBox[{"a", "+", "L"}]}], "}"}]}], "]"}]}], ";"}], "*)"}], 
       RowBox[{"ufun", "=", 
        RowBox[{"NDSolve", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"-", 
               RowBox[{"Laplacian", "[", 
                RowBox[{
                 RowBox[{"u", "[", "x", "]"}], ",", 
                 RowBox[{"{", "x", "}"}]}], "]"}]}], "-", 
              RowBox[{"2", "*", "I", "*", "k", "*", 
               RowBox[{"D", "[", 
                RowBox[{
                 RowBox[{"u", "[", "x", "]"}], ",", "x"}], "]"}]}], "+", 
              RowBox[{
               SuperscriptBox["k", "2"], "*", 
               RowBox[{"u", "[", "x", "]"}]}], "+", 
              RowBox[{
               RowBox[{"V", "[", 
                RowBox[{"x", ",", "\[Lambda]1"}], "]"}], "*", 
               RowBox[{"u", "[", "x", "]"}]}]}], "\[Equal]", 
             RowBox[{
              RowBox[{"vals", "[", 
               RowBox[{"[", "n", "]"}], "]"}], "*", 
              RowBox[{"u", "[", "x", "]"}]}]}], ",", 
            RowBox[{
             RowBox[{"u", "[", "a", "]"}], "\[Equal]", 
             RowBox[{"u", "[", 
              RowBox[{"a", "+", "L"}], "]"}]}], ",", 
            RowBox[{
             RowBox[{"u", "[", 
              RowBox[{"a", "+", 
               RowBox[{"0.0", "*", "L"}]}], "]"}], "\[Equal]", "0.2"}]}], 
