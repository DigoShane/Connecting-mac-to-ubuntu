              RowBox[{"a", "+", "L"}]}], "}"}]}], "]"}], "]"}]]}], ";", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"myperiodic", "[", 
          RowBox[{
           FractionBox[
            RowBox[{"Evaluate", "[", 
